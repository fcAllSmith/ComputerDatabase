package com.excilys.computerdb.fconsigny.app;

public class AppLauncher {

	public static void main(String[] args) {
		System.out.println("-- Welcom  to Computer Database");
		System.out.println("1 - Select all companies");
		System.out.println("2 - Select company with an id");
		System.out.println("3 - Select all computers");
		System.out.println("4 - Select a computer with an id");
	}
	
	public void userCommand(String strInput){
		int cmd = Integer.parseInt(strInput); 
		switch(cmd){
		case 1:
			break; 
		case 2: 
			break; 
		case 3
			break;
		case 4: 
			break;
		default:
			
			break; 
		}
	}
	
	public static void showText(String strToPrint){
		System.out.println(strToPrint);
	}
}
